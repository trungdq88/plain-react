{
  "name": "react-admin",
  "version": "0.0.0",
  "description": "Boilerplate for Admin UI application built on React and Flux",
  "main": "src/app.jsx",
  "scripts": {
    "build": "NODE_ENV=production npm run webpack",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "env": "env",
    "lint": "eslint --ext .jsx ./src && echo No linting errors.",
    "prebuild": "npm run clean",
    "pretest": "bower install && npm install && npm run lint",
    "pretest-travis": "npm install && npm run lint",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "test": "NODE_ENV=test karma start",
    "test-travis": "NODE_ENV=test karma start --single-run",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  },
  "repository": {
    "type": "git+git@github.com:trungdq88/plain-react.git",
    "url": "https://github.com/trungdq88/plain-react"
  },
  "author": "Trung Dinh Quang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/trungdq88/plain-react/issues"
  },
  "config": {
    "buildDir": "./build",
    "devHost": "localhost",
    "devPort": 8763
  },
  "homepage": "https://github.com/trungdq88/plain-react",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.1.4",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.1.0",
    "babel-plugin-rewire": "^0.1.8",
    "babel-preset-es2015": "^6.1.4",
    "babel-preset-react": "^6.1.4",
    "babel-runtime": "^5.6.15",
    "chai": "^3.4.1",
    "css-loader": "^0.22.0",
    "file-loader": "^0.8.4",
    "glob": "^5.0.15",
    "imports-loader": "^0.6.4",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.1",
    "karma-coverage": "^0.5.3",
    "karma-coveralls": "^1.1.2",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.0.2",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sinon": "^1.0.4",
    "karma-source-map-support": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.5.1",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "lodash": "^3.10.1",
    "mocha": "^2.2.5",
    "mocha-loader": "^0.7.1",
    "opn": "^3.0.2",
    "phantomjs": "^1.9.17",
    "react-addons-test-utils": "^0.14.2",
    "react-hot-loader": "^1.3.0",
    "rimraf": "^2.4.3",
    "script-loader": "^0.6.1",
    "sinon": "^1.15.4",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "autoprefixer": "^6.0.3",
    "babel-polyfill": "^6.1.4",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^1.0.0",
    "eslint-plugin-react": "^3.5.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "postcss-loader": "^0.8.0",
    "webpack-dev-server": "^1.12.0"
  },
  "dependencies": {
    "classnames": "^2.1.5",
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  }
}
