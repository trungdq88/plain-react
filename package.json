{
  "name": "plain-redux",
  "version": "0.0.0",
  "description": "Plain react / redux boilerplate for static web app",
  "main": "sr/app.jsx",
  "scripts": {
    "staging": "NODE_ENV=staging npm run webpack",
    "production": "NODE_ENV=production npm run webpack",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "env": "env",
    "lint": "eslint --ext .js ./src/js && echo No linting errors.",
    "prebuild": "npm run clean",
    "prestart": "npm install",
    "pretest": "npm install && npm run lint",
    "pretest-travis": "npm install && npm run lint",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "test": "NODE_ENV=test karma start",
    "test-travis": "NODE_ENV=test karma start --single-run",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trungdq88/plain-redux.git"
  },
  "author": "SSS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/trungdq88/plain-redux/issues"
  },
  "config": {
    "buildDir": "./build",
    "devHost": "localhost",
    "devPort": 8818
  },
  "homepage": "https://github.com/trungdq88/plain-redux",
  "devDependencies": {
    "babel-core": "^5.8.3",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.1",
    "babel-plugin-rewire": "^0.1.8",
    "babel-runtime": "^5.6.15",
    "css-loader": "^0.20.2",
    "rimraf": "^2.4.3",
    "webpack": "^1.12.2",
    "opn": "^3.0.2",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "less-loader": "^2.2.1",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.0",
    "template-html-loader": "0.0.3",
    "url-loader": "^0.5.6",
    "webpack-dev-server": "^1.12.0",
    "lodash": "^3.10.1",
    "less": "^2.5.3"
  },
  "dependencies": {
    "autoprefixer": "^6.0.3",
    "classnames": "^2.1.5",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.5.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "postcss-loader": "^0.7.0",
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  }
}
